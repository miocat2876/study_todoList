<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.famback.fam.user.mapper.UserAuthorityMapper">
    <!-- 방참가  -->
    <insert id="createUserAuthority" parameterType="userAuthorityDomain">
        INSERT INTO USER_AUTHORITY_TAB
            (USER_FK, AUTHORITY_CODE)
        VALUES(#{userFk}, #{authorityCode});
    </insert>

</mapper>